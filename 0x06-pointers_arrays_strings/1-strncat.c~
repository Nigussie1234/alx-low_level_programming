#include "holberton.h"
/**
 *_strncat - concatenates two strings
 * @dest: destination
 * @src: source
 * @n: n is an integer
 *
 * Return - pointer to the resulting string dest
 */

char *_strncat(char *dest, char *src, int n);
{
char i = 0;
char j;
while (dest[n] != '\0')
{
n++;
}
for (m = 0; src[m] != '\0'; m++; n++)
{
dest[n] = src[m];
}
dest[m] = '\0';
return (dest);
}
